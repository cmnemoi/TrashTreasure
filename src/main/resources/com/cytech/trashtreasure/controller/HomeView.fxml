<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cytech.trashtreasure.controller.HomeController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox layoutX="138.0" layoutY="116.0" prefHeight="118.0" prefWidth="364.0" spacing="10.0">
               <children>
                  <HBox prefHeight="21.0" prefWidth="421.0">
                     <children>
                        <Label text="Nom d'utilisateur">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                           <padding>
                              <Insets right="10.0" />
                           </padding>
                        </Label>
                        <TextField fx:id="usernameField" />
                     </children>
                  </HBox>
                  <HBox prefHeight="27.0" prefWidth="267.0">
                     <children>
                        <Label text="Mot de passe " />
                        <PasswordField fx:id="passwordField">
                           <HBox.margin>
                              <Insets left="31.0" />
                           </HBox.margin>
                        </PasswordField>
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets left="30.0" top="35.0" />
               </padding>
            </VBox>
            <ButtonBar layoutX="388.0" layoutY="343.0" prefHeight="43.0" prefWidth="238.0">
              <buttons>
                  <Button fx:id="registerButton" mnemonicParsing="false" text="S'inscrire" />
